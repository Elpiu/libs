{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-dynamic-config": {
      "projectType": "library",
      "root": "projects/ng-dynamic-config",
      "sourceRoot": "projects/ng-dynamic-config/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-dynamic-config/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-dynamic-config/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-dynamic-config/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-dynamic-config/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "c418271b-c19b-4a50-83e9-149a75ffd3f6"
  }
}
